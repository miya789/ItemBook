<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<div class="container">
    <% @users.each do |user| %>
        <% if user.category == "admin" %>
            <div class="user-info admin">
                <%= link_to("#{user.name}", "/users/#{user.id}") %>
            </div>
        <% elsif %>
            <div class="user-info">
                <%= link_to("#{user.name}", "/users/#{user.id}") %>
             </div>
        <% end %>
    <% end %>
</div>